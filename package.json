{
  "name": "banlogger",
  "version": "1.1.1",
  "description": "Logger",
  "main": "index.js",
  "bin": "index.js",
  "author": "NekoSuneVR",
  "scripts": {
    "test": "electron .",
    "build": "electron-builder",
    "build:win": "electron-builder --win"
  },
  "build": {
    "copyright": "Copyright Â© 2024-2025 NekoSuneVR",
    "appId": "com.nekosunevr.banlogger",
    "productName": "BanLogger",
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "license": "license.txt",
      "oneClick": false
    },
    "win": {
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "icon": "assets/icon.ico",
      "files": [
        "main.js",
        "web/**/*",
        "preload.js",
        "index.js",
        "APIClient/**",
        "assets/**",
        "AvatarUpload/**",
        "Configfiles/**",
        "functions/**",
        "models/**",
        "playercounter/**",
        "SafetyProtection/**",
        "Splitlogjsonpart/**",
        "VRChatLogUserID/**",
        "vrcx/**",
        "webhook/**"
      ],
      "target": [
        "nsis",
        "msi"
      ]
    },
    "appx": {
      "backgroundColor": "#ffffff",
      "displayName": "BanLogger",
      "publisher": "CN=NekoSuneVR",
      "publisherDisplayName": "NekoSuneVR",
      "identityName": "com.nekosunevr.banlogger"
    },
    "directories": {
      "output": "dist",
      "buildResources": "assets"
    },
    "extraFiles": [
      {
        "from": "assets/alert.wav",
        "to": "assets/alert.wav"
      }
    ]
  },
  "license": "MIT",
  "dependencies": {
    "axios": "0.27.2",
    "bottleneck": "^2.19.5",
    "child_process": "^1.0.2",
    "chokidar": "^3.6.0",
    "clc": "^1.0.2",
    "cli-color": "^2.0.4",
    "configstore": "^7.0.0",
    "find-process": "^1.4.7",
    "fuzzyset": "^1.0.7",
    "music-metadata": "^10.7.0",
    "node-fetch": "2.7.0",
    "p-queue": "^6.6.2",
    "path": "^0.12.7",
    "pcm-volume": "^1.0.0",
    "play-sound": "^1.1.6",
    "readline": "^1.3.0",
    "say": "^0.16.0",
    "sequelize": "^6.37.3",
    "speaker": "^0.5.5",
    "sqlite3": "^5.1.7",
    "vrnotications": "^1.2.0"
  },
  "devDependencies": {
    "electron": "^31.3.1",
    "electron-builder": "^25.1.8"
  }
}
